var x : int = 1;
if(x == 1) {
    x += 8 + 2;
}
println(x);
print("While Loop : ");
var w = 0;
while(w < 10) {
    print(w);
    w++;
}
println(' ');
print("Do While Loop : ");
var dw = 0;
do {
    print(dw);
    dw++;
} while(dw < 10);
println(' ');
print("For Loop : ");
for(var j = 0; j < 10; j++) {
    print(j);
}
println(' ')