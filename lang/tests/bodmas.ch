import "test.ch"

func main() {
    test("4 + 2 / 2 == 5", []() => {
        return (4 + 2 / 2) == 5;
    });
    test("4 / 2 + 2 == 4", []() => {
        return (4 / 2 + 2) == 4;
    });
    test("3 * (4 + 2) - 8 / 2 == 5", []() => {
        return (4 + 2 / 2) == 5;
    });
    test("3 * (4 + 2) - 8 / 2 == 14", []() => {
        return (3 * (4 + 2) - 8 / 2) == 14;
    });
    test("8 / (2 + 2) * 3 - 1 == 5", []() => {
        return (8 / (2 + 2) * 3 - 1) == 5;
    });
    test("(5 + 3) * 2 - 4 / (1 + 1) == 14", []() => {
        return ((5 + 3) * 2 - 4 / (1 + 1)) == 14;
    });
}