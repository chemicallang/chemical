if(!def.windows) {

    /** @typedef ssize_t
     * Signed size type, used for byte counts or error indications.
     */
    public type ssize_t = long;

    /**
     * @typedef off_t
     * File offset type, used for file sizes and positions.
     */
    public type off_t = long;

    /**
     * @typedef pid_t
     * Process ID type.
     */
    public type pid_t = int;

}